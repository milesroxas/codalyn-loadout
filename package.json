{
  "author": {
    "name": "Miles Roxas",
    "url": "https://milesroxas.com/"
  },
  "bugs": {
    "url": "https://github.com/milesroxas/codalyn-loadout/issues"
  },
  "dependencies": {
    "@finsweet/ts-utils": "^0.40.0",
    "swiper": "^12.0.3"
  },
  "description": "Extending Webflow with custom scripts.",
  "devDependencies": {
    "@biomejs/biome": "2.2.6",
    "@finsweet/tsconfig": "^1.4.2",
    "@playwright/test": "^1.56.1",
    "cross-env": "^7.0.3",
    "esbuild": "^0.24.2",
    "typescript": "^5.9.3"
  },
  "engines": {
    "pnpm": ">=10"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/finsweet/developer-starter#readme",
  "keywords": [],
  "license": "ISC",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "name": "@finsweet/developer-starter",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/finsweet/developer-starter.git"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production node ./bin/build.js",
    "check": "biome check ./src && tsc --noEmit",
    "check:fix": "biome check --write ./src && tsc --noEmit",
    "dev": "cross-env NODE_ENV=development node ./bin/build.js",
    "format": "biome format --write ./src",
    "lint": "biome lint ./src",
    "lint:fix": "biome lint --write ./src",
    "push": "git restore dist/ 2>/dev/null || true && git pull --rebase origin main && git push origin main",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "update": "pnpm update -i -L -r"
  },
  "type": "module",
  "version": "0.0.0"
}
